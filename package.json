{
  "name": "cape-editor",
  "description": "CAPE Editor",
  "author": "Kai Curry <kai@cape.io> (http://github.com/webmasterkai)",
  "contributors": [
    {
      "name": "Tim Lupfer",
      "email": "tim@cape.io"
    }
  ],
  "license": "MIT",
  "version": "0.0.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/cape-io/cape-editor"
  },
  "bugs": {
    "url": "https://github.com/cape-io/cape-editor/issues"
  },
  "homepage": "https://edit.cape.io",
  "keywords": [
    "cape",
    "react",
    "isomorphic",
    "universal",
    "redux",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "node ./node_modules/better-npm-run start-prod",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "webpack --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "karma start"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3002
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "babel": "5.8.23",
    "babel-plugin-typecheck": "1.3.0",
    "body-parser": "1.14.1",
    "compression": "1.6.0",
    "express": "4.13.3",
    "express-session": "1.11.3",
    "file-loader": "0.8.4",
    "history": "1.12.5",
    "http-proxy": "1.x",
    "less": "2.5.3",
    "less-loader": "2.2.1",
    "lodash.find": "^3.2.1",
    "lodash.isarray": "^3.0.4",
    "lodash.mapvalues": "^3.0.1",
    "lodash.pluck": "^3.1.2",
    "lru-memoize": "1.0.0",
    "map-props": "1.0.0",
    "moment": "2.x",
    "multireducer": "1.0.2",
    "piping": "0.3.0",
    "pretty-error": "1.2.0",
    "query-string": "3.0.0",
    "react": "^0.14.0",
    "react-bootstrap-datetimepicker": "0.0.21",
    "react-document-meta": "2.0.x",
    "react-dom": "0.14.0",
    "react-inline-css": "2.0.0",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-rc3",
    "react-select": "0.8.x",
    "react-textarea-autosize": "3.1.x",
    "react-widgets": "3.0.x",
    "redux": "3.0.x",
    "redux-form": "2.4.x",
    "serialize-javascript": "1.1.2",
    "serve-favicon": "2.3.0",
    "serve-static": "1.10.0",
    "socket.io": "1.3.7",
    "socket.io-client": "1.3.7",
    "superagent": "1.4.0",
    "url-loader": "0.5.6",
    "webpack-isomorphic-tools": "1.0.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "better-npm-run": "0.0.x",
    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.9",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.3",
    "concurrently": "^0.1.1",
    "css-loader": "^0.21.0",
    "eslint": "1.7.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.1",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "json-loader": "^0.5.3",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
